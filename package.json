{
  "name": "mytests",
  "description": "mytests",
  "author": "Matveev Savva",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/testHelper.js  --recursive",
    "test:watch": "npm run test -- --watch",
    "set-prod": "set NODE_ENV=production",
    "set-dev": "set NODE_ENV=development",
    "start-dev-server": "npm run set-dev && webpack-dev-server",
    "development": "npm run set-dev && webpack --config webpack.config.js",
    "production": "npm run set-prod && webpack --config webpack.production.config.js && node pindex.js && npm run set-dev"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "camelcase": "^3.0.0",
    "classlist-polyfill": "^1.0.3",
    "classnames": "^2.2.5",
    "keymirror": "^0.1.1",
    "lodash": "^4.15.0",
    "moment": "^2.17.1",
    "normalizr": "^3.2.2",
    "query-string": "^4.3.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-input-slider": "^1.6.0",
    "react-numeric-input": "^2.0.7",
    "react-onclickoutside": "^5.7.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.24.0",
    "eslint": "^3.4.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "node-sass": "^3.8.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
